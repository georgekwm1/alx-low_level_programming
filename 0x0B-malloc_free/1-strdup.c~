#include "main.h"
#include <stdio.h>
#include <stdlib.h>
#include <stdlib.h>
/**
 * _strdup - returns a pointer to a newly allocated space in memory
 *
 * Description: 'the program's description'
 * @str: describe the parameter
 *
 * Return: Always 0 (Success)
 */
char *_strdup(char *str)
{
int size;
char new;
new = str[];
size = strlen(new)

new = malloc(size * sizeof(char))
}
